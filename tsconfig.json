{
  "compilerOptions": {
    "esModuleInterop": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "sourceMap": true,
    "plugins": [
      {
        "name": "typescript-plugin-css-modules"
      }
    ],
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "lib": [
      "DOM",
      "ES2019"
    ],
    "strictBindCallApply": true
  }
}